#include <bits/stdc++.h>
using namespace std;
int check(vector<int>& nums,vector<int>& numsDivide){
	sort(nums.begin(),nums.end());
        if(nums[0]==1){
            return 0;
        }
        int g=0;
        for(auto &i:numsDivide){
            g=__gcd(g,i);
            // cout<<g<<endl;
        }
        if(g==1 || g<nums[0]){
            return -1;
        }
        // int i=0;
        for(int i=0;i<nums.size();i++){
            if(g%nums[i]==0){
                return i;
            }
        }
        return -1;
}
int main() 
{
  	int n;
	cin>>n;
  	vector<int>nums(n); 
  	
  	for(int i=0;i<n;i++){
  		cin>>nums[i];
  	}
  	int m;
  	cin>>m;
  	vector<int>numsDivide(m);
  	for(int i=0;i<m;i++){
  		cin>>numsDivide[i];
  	}
  	int temp=check(nums,numsDivide);
  	cout<<temp;
   return 0;
}
